{
  "name": "@exchange-frontend/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "pre-commit": "npm run lint-all && npm run build",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "build:admin": "nx build admin",
    "build:client": "nx build client",
    "start:admin": "nx serve admin --port 4200",
    "start:client": "nx serve client --port 4201",
    "start": "npm run build:admin && npm run serve:admin && npm run build:client && npm run serve:client",
    "lint": "nx workspace-lint && ng lint",
    "format": "prettier --write 'apps/**/*.ts'",
    "test:client": "nx test --project=client --codeCoverage",
    "test:admin": "nx test --project=admin --codeCoverage",
    "update": "nx migrate latest"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.0.0",
    "@angular-devkit/core": "~16.0.0",
    "@angular-devkit/schematics": "~16.0.0",
    "@angular-eslint/eslint-plugin": "~16.0.0",
    "@angular-eslint/eslint-plugin-template": "~16.0.0",
    "@angular-eslint/template-parser": "~16.0.0",
    "@angular/cli": "~16.0.0",
    "@angular/compiler-cli": "~16.0.0",
    "@angular/language-service": "~16.0.0",
    "@nrwl/angular": "^16.3.2",
    "@nrwl/storybook": "^16.3.2",
    "@nx/cypress": "16.3.2",
    "@nx/eslint-plugin": "16.3.2",
    "@nx/jest": "16.3.2",
    "@nx/js": "16.3.2",
    "@nx/linter": "16.3.2",
    "@nx/storybook": "16.3.2",
    "@nx/web": "16.3.2",
    "@nx/workspace": "16.3.2",
    "@schematics/angular": "~16.0.0",
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/angular": "^7.0.9",
    "@storybook/core-server": "^7.0.9",
    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cypress": "^12.11.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-preset-angular": "~13.1.0",
    "lint-staged": "^13.2.2",
    "nx": "16.3.2",
    "nx-cloud": "latest",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslint": "^6.1.3",
    "typescript": "~5.0.2"
  },
  "dependencies": {
    "@angular/animations": "~16.0.0",
    "@angular/common": "~16.0.0",
    "@angular/compiler": "~16.0.0",
    "@angular/core": "~16.0.0",
    "@angular/forms": "~16.0.0",
    "@angular/platform-browser": "~16.0.0",
    "@angular/platform-browser-dynamic": "~16.0.0",
    "@angular/router": "~16.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "lint-staged": {
    "**/*.ts": [
      "nx format:write --base=origin/main --uncommitted",
      "git add"
    ],
    "**/*.{html,css}": [
      "prettier --write",
      "git add"
    ]
  }  
}
